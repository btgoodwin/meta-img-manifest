<?xml version="1.0" encoding="UTF-8"?>

<manifest>
    <default revision="morty" remote="origin"/>
    <remote name="origin" fetch="." />
    <remote name="yocto" fetch="git://git.yoctoproject.org" />
    <remote name="openembedded" fetch="git://github.com/openembedded" />
    
    <project name="poky"              remote="yocto"/> <!-- path="poky" implicitly -->
    <project name="meta-openembedded" remote="openembedded" path="poky/meta-openembedded" />
    <!-- remote="origin" default -->
    <project name="meta-img"          path="poky/meta-img" />
</manifest>